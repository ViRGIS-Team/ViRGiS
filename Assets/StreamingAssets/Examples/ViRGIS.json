{
    "pipeline": [
        {
            "type":"readers.gdal",
            "filename":"../../../../test_data/EC_DML.ers",
            "header":"mag"
        },
        {
            "type":"filters.range",
            "limits":"mag[0:50000]"
        },
        {
            "type": "filters.ferry",
            "dimensions": "=>Z"
        },
        {
            "type":"filters.hag_dem",
            "raster": "../../../../test_data/hope_dem.tif"
        },
        {
            "type":"filters.reprojection",
            "in_srs": "epsg:27700",
            "out_srs":"+proj=tmerc +datum=WGS84 +units=m +no-defs +lat_0=53.316200583246534 +lon_0=-1.791218230167096"
        },
        {
            "type":"filters.colorinterp",
            "ramp":"heat_map",
            "dimension":"mag"
          },
        {
            "type":"writers.text",
            "filename":"../../../../test_data/EC_DML.txt"
        }
    ]
}
